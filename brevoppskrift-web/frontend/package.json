{
  "name": "brevoppskrift-web-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "tsc --noemit",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:write": "biome check --write .",
    "lint-and-check-types": "${npm_execpath} run format && ${npm_execpath} run lint && ${npm_execpath} run stylelint && ${npm_execpath} run check-types",
    "preview": "vite preview",
    "stylelint:fix": "stylelint --fix '**/*.{tsx,css}'",
    "stylelint": "stylelint '**/*.{tsx,css}'"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@navikt/aksel-icons": "8.5.2",
    "@navikt/ds-css": "8.5.2",
    "@navikt/ds-react": "8.5.2",
    "@tanstack/react-query": "5.90.21",
    "@tanstack/react-router": "1.162.1",
    "axios": "1.13.5",
    "lodash": "4.17.23",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@navikt/aksel-stylelint": "8.5.2",
    "@tanstack/react-query-devtools": "5.91.3",
    "@tanstack/router-devtools": "1.162.1",
    "@tanstack/router-plugin": "1.162.1",
    "@types/lodash": "4.17.23",
    "@types/node": "25.3.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "globals": "17.3.0",
    "lint-staged": "16.2.7",
    "postcss-styled-syntax": "0.7.1",
    "stylelint": "17.1.1",
    "stylelint-config-standard": "40.0.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  }
}
