{
  "name": "skribenten-web-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --component -b chrome",
    "preview": "vite preview",
    "check-types": "tsc --noemit",
    "test": "vitest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "stylelint": "stylelint '**/*.{tsx,css}'",
    "stylelint:fix": "stylelint --fix '**/*.{tsx,css}'",
    "lint-and-check-types": "npm run check-types && npm run lint && npm run stylelint",
    "tsc:watch": "tsc --watch"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@hookform/resolvers": "3.3.4",
    "@navikt/aksel-icons": "6.2.0",
    "@navikt/ds-css": "6.2.0",
    "@navikt/ds-react": "6.2.0",
    "@tanstack/react-query": "5.28.4",
    "@tanstack/react-router": "1.20.1",
    "axios": "1.7.4",
    "date-fns": "3.6.0",
    "immer": "10.0.4",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.1",
