{
  "name": "skribenten-web-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "cypress": "cypress open",
    "cypress:e2e": "cypress run -b chrome",
    "cypress:component": "cypress run --component -b chrome",
    "preview": "vite preview",
    "check-types": "tsc --noemit",
    "test": "vitest --config ./vite.config.ts",
    "verify": "${npm_execpath} run lint-and-check-types && vitest run --config ./vite.config.ts && ${npm_execpath} run cypress:component && ${npm_execpath} run cypress:e2e",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "stylelint": "stylelint '**/*.{tsx,css}'",
    "stylelint:fix": "stylelint --fix '**/*.{tsx,css}'",
    "lint-and-check-types": "${npm_execpath} run check-types && ${npm_execpath} run lint && ${npm_execpath} run stylelint",
    "tsc:watch": "tsc --watch"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@hookform/resolvers": "5.2.2",
    "@navikt/aksel-icons": "8.1.0",
    "@navikt/ds-css": "8.1.0",
    "@navikt/ds-react": "8.1.0",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-router": "^1.157.5",
    "axios": "1.13.2",
    "date-fns": "4.1.0",
    "dompurify": "3.3.1",
    "fuse.js": "7.1.0",
    "immer": "11.1.3",
    "lodash": "4.17.23",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "react-pdf": "10.3.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@navikt/aksel-stylelint": "8.1.0",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@tanstack/react-query-devtools": "5.91.2",
    "@tanstack/react-router-devtools": "^1.157.5",
    "@tanstack/router-vite-plugin": "1.157.5",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/lodash": "4.17.23",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "@vitejs/plugin-react": "5.1.2",
    "cypress": "15.9.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.15.4",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "17.1.0",
    "happy-dom": "20.3.7",
    "jsdom": "27.4.0",
    "postcss-styled-syntax": "0.7.1",
    "prettier": "3.7.4",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "overrides": {
    "cypress": {
      "qs": ">=6.14.1"
    }
  }
}
