    "jsdom": "24.0.0",
    "postcss-styled-syntax": "0.6.4",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.4.2",
    "vite": "5.4.6",
    "vitest": "1.4.0"
