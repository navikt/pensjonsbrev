    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/lodash": "4.17.0",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "cypress": "13.8.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
